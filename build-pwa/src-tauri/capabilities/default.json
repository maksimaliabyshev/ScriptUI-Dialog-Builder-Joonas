{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "window-state:allow-restore-state",
    "window-state:allow-save-window-state",
    "fs:read-files",
    "fs:write-files",
    "fs:read-all",
    "fs:write-all",
    "upload:allow-download",
    "upload:allow-upload",
    "dialog:allow-ask",
    "dialog:allow-save"
  ]
}